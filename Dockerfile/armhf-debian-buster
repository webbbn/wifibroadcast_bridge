FROM arm32v7/debian:buster

# Ensure that dpkg doesn't prompt for input
ENV DEBIAN_FRONTEND noninteractive

# Install dependencies
RUN apt-get update
RUN apt-get install -y git libboost-all-dev libpcap-dev python3-pyudev libpcap0.8-dev python3-pip python3-setuptools python3-wheel cmake build-essential liblog4cpp5-dev
